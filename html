<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Rotating Link</title>
</head>
<body>
    <a href="#" id="rotatingLink">Click me!</a>

    <script>
        const links = [
            'https://www.example-a.com',
            'https://www.example-b.com',
            'https://www.example-c.com',
            'https://www.example-d.com'
        ];
        let currentIndex = 0;

        document.getElementById('rotatingLink').addEventListener('click', function(e) {
            e.preventDefault();
            window.location.href = links[currentIndex];
            currentIndex = (currentIndex + 1) % links.length;
        });
    </script>
</body>
</html>
